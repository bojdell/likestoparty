//- jade index.jade -O index.json -P
doctype html
html
  head
    title Syman Emoji Archive
    link(rel='stylesheet', type='text/css', href='/syman/style.css')
    link(href='https://fonts.googleapis.com/css?family=Cutive+Mono', rel='stylesheet', type='text/css')
    link(rel='icon' type='image/png' href='/img/syman-emoji/symanface.png')
    meta(name='viewport', content='width=device-width, initial-scale=1')
  body
    h1 Syman Emoji Archive
    #emoji-container
      for emoji in emojis
        .emoji-row
          img(src='/img/syman-emoji/' + emoji)
          p!= ':' + emoji.split('.')[0].replace('_', '_<wbr>') + ':'

    p Last updated 1.17.16
